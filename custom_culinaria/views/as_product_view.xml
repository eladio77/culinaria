<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="culinaria_product_tree_inherited" model="ir.ui.view">
        <field name="name">culinaria_product_tree_inherited</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_tree_view" />
        <field name="arch" type="xml">
            <field name="categ_id" position="after">
                <field name="taxes_id.name" string='Imp. Cliente' />
            </field>

            <field name="responsible_id" position="attributes">
                <attribute name="invisible">1</attribute>
            </field>
        </field>

    </record>
    

</odoo>

